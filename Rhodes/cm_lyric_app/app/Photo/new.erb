<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Search Photos</h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @photo.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="photo[name]" class="fieldLabel">Photo</label>
            <input type="text" id="photo[name]" name="photo[name]" <%= placeholder("Name") %> />
          </div>
      

      <input type="submit" value="Search"/>
    </form>
  </div>

</div>
